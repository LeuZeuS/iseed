{
    "name": "schubu/iseed",
    "description": "This is a fork of the original package Orangehill/Iseed by Tihomir Opacic (tihomir.opacic@SchuBudev.com). Generate a new Laravel database seed file based on data from the existing database table.",
    "keywords": ["laravel", "generators", "seed", "artisan"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Peter Schulze-Buxloh",
            "email": "peter@schu-bu.de"
        },
        {
            "name": "Tihomir Opacic",
            "email": "tihomir.opacic@SchuBudev.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0.0",
        "illuminate/filesystem": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-0": {
            "SchuBu\\Iseed": "src/"
        },
        "classmap": [
            "src/SchuBu/Iseed/Exceptions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SchuBu\\Iseed\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SchuBu\\Iseed\\IseedServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
